/* Multiple Inheritance
In multiple inheritance a class can inherit features of multiple classes.
syntax
class child class name : public parent1 class name , public parent class*/
#include<iostream>
using namespace std;
class Parent1{
    public : void Fun1()
    {
        cout<<"Paremt1 class Function";
    }
};
class Parent2{
    public : void Fun2()
    {
        cout<<"Parent2 class Function";
    }
};
class child : public Parent1 , public Parent2{
    public : void Fun3()
    {
        cout<<"child class Fumction";
    }
};
int main ()
{
    child obj;
    obj.Fun1();
    obj.Fun2();
    obj.Fun3();
    return 0;
}
